{
  "name": "nitro-app",
  "private": true,
  "scripts": {
    "prepare": "nitro prepare",
    "build": "nitro build",
    "build:base": "bun run switch:base && bun run migrate && bun run build",
    "build:vercel": "bun run switch:vercel && bun run migrate && bun run build",
    "switch:base": "rm nitro.config.ts && cp nitro.config-base.ts nitro.config.ts",
    "switch:vercel": "rm nitro.config.ts && cp nitro.config-vercel.ts nitro.config.ts",
    "migrate": "drizzle-kit migrate",
    "dev": "nitro dev",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.7.1",
    "@testcontainers/valkey": "^11.7.1",
    "@types/clamp": "^1.0.3",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.5",
    "h3": "^1.15.4",
    "nitro-test-utils": "^0.9.4",
    "nitropack": "^2.12.7",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@vercel/blob": "^2.0.0",
    "@vercel/edge-config": "^1.4.0",
    "aws4fetch": "^1.0.20",
    "clamp": "^1.0.1",
    "deep-equal": "^2.2.3",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "eventemitter3": "^5.0.1",
    "pg": "^8.16.3",
    "tinyrainbow": "^3.0.3",
    "zod": "^4.1.12"
  }
}
